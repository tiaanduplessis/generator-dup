{
  "name": "<%= moduleName %>",
  "version": "1.0.0",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubUsername %>/<%= moduleName %>.git"
  },
  "homepage": "https://github.com/<%= githubUsername %>/<%= moduleName %>",
  "bugs": "https://github.com/<%= githubUsername %>/<%= moduleName %>/issues",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },  <% if (cli) { %>
  "bin": [
    "<%= moduleName %>": "cli.js"
  ],<% } %>
  "scripts": {
    "start": "npm run dev",
    "dev": "npm test -- -w",
  	"lint": "standard --verbose",
    "lint:fix": "standard --fix --verbose",
    "pretest": "npm run lint:fix",
    "test": "mocha"
  },
  "keywords": [
  <% for(let i= 0; i< keywords.length; i++) {%>"<%= keywords[i] %>",<% } %>
  "<%= moduleName %>"
  ],
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "standard": "^8.6.0"
  }
}
